<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toStartOf="@+id/linearLayout9"
            motion:layout_constraintTop_toTopOf="@+id/textView13"
            motion:layout_constraintBottom_toBottomOf="@+id/textView13"
            android:layout_width="wrap_content"
            android:id="@+id/membership_kind_id"
            android:layout_marginRight="60dp"
            android:layout_marginEnd="60dp" />
        <Constraint
            android:id="@+id/member_name_id"
            motion:layout_constraintEnd_toStartOf="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/textView9"
            android:layout_marginEnd="15dp"
            motion:layout_constraintTop_toTopOf="@+id/textView9"
            android:layout_marginRight="15dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/member_number_id2"
            motion:layout_constraintEnd_toEndOf="@+id/member_name_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/textView10"
            motion:layout_constraintTop_toTopOf="@+id/textView10"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>